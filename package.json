{
    "name": "medusa-plugin-cart-related",
    "version": "1.0.0",
    "description": "A plugin that adds a route to get related products to the products currently in your medusa cart",
    "main": "index.js",
    "scripts": {
        "build": "babel src -d .",
        "prepare": "cross-env NODE_ENV=production npm run build",
        "watch": "babel -w src --out-dir . --ignore **/__tests__"
    },
    "keywords": [
        "medusa",
        "cart-related",
        "medusa-plugin"
    ],
    "peerDependencies": {
        "medusa-interfaces": "1.x"
    },
    "author": "Blechlawine",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.4",
        "cross-env": "^7.0.3"
    },
    "dependencies": {
        "express": "^4.17.1",
        "medusa-core-utils": "^1.1.30",
        "typeorm": "^0.2.41"
    }
}
